/** id type */
type id = integer;

/** @title User */
type user = struct {
    /** unique id of the user */
    id: id,
    name: string?,
    type: enum {"admin", "writer", "reader"},
    SNSs: [string],
};
/**
 * @title Group
 * this expresses a group of users
 */
type group = struct {
  id: id,
  members: [user],
};
